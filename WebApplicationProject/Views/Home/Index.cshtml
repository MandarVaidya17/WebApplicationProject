@{
    ViewData["Title"] = "Home Page";
}

<h3>
   &copy; @TempData["company"]
</h3>

<h3>
    @ViewData["message"]
</h3>

<p>
    @ViewData["message"]
</p>

<p>
    ViewData["message"]
</p>


<h3>Cities List</h3>
@if (ViewData["cities"] != null)
{
    <ol>
        @foreach(string item in (IEnumerable<string>)ViewData["cities"])
        {
            <li>
                @item
            </li>
        }
    </ol>
}

<p>-----------------------------------</p>
<h3>This is viewbag</h3>
<p>
    @ViewBag.Msg
</p>

<ol>
    @foreach(string item in ViewBag.Cities)
    {
        <li>
            @item
        </li>
    }
</ol>
